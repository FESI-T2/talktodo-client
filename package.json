{
  "name": "talktodo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "cm": "cz",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headless": "cypress run --headless",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress:run",
    "e2e:open": "start-server-and-test dev http://localhost:3000 cypress:open",
    "mock-server": "tsx mock-server.ts",
    "mock:dev": "concurrently \"npm run dev\" \"npm run mock-server\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@suspensive/react": "^3.3.2",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "lucide-react": "^0.525.0",
    "next": "15.3.4",
    "react": "^19.0.0",
    "react-day-picker": "^9.8.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.60.0",
    "react-tooltip": "^5.29.1",
    "tailwind-merge": "^3.3.1",
    "ts-pattern": "^5.7.1",
    "usehooks-ts": "^3.1.1",
    "zod": "^4.0.5",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@eslint/eslintrc": "^3",
    "@storybook/addon-a11y": "^9.0.14",
    "@storybook/addon-docs": "^9.0.14",
    "@storybook/addon-onboarding": "^9.0.14",
    "@storybook/addon-vitest": "^9.0.14",
    "@storybook/nextjs-vite": "^9.0.14",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.1",
    "concurrently": "^9.2.0",
    "cypress": "^14.5.1",
    "cz-customizable": "^7.4.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-storybook": "^9.0.14",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "jest": "^30.0.3",
    "jest-environment-jsdom": "^30.0.2",
    "msw": "^2.10.3",
    "playwright": "^1.53.2",
    "prettier": "^3.6.2",
    "start-server-and-test": "^2.0.12",
    "storybook": "^9.0.14",
